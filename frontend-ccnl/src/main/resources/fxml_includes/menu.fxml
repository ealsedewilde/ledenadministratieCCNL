          <items>
            <MenuItem onAction="#makeReminderLettersDD" mnemonicParsing="false"
              text="Leden met Automatische Incasso" />
            <MenuItem onAction="#makeReminderLettersBT" mnemonicParsing="false"
              text="Leden met Overboeking" />
          </items>
        </Menu>
        <MenuItem onAction="#yearly" mnemonicParsing="false" text="Jaarovergang" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Beheer">
      <items>
        <MenuItem onAction="#manageSepaAuthorizationForm" mnemonicParsing="false"
          text="SEPA-machtiging formulier" />
        <MenuItem onAction="#manageDocumentTemplates" mnemonicParsing="false"
          text="Beheer document templates" />
        <MenuItem onAction="#backupDatabase" mnemonicParsing="false" text="Backup Database" />
        <MenuItem onAction="#restoreDatabase" mnemonicParsing="false"
          text="Restore Database" />
        <SeparatorMenuItem mnemonicParsing="false" />
        <MenuItem onAction="#importFromExcel" mnemonicParsing="false"
          text="Import uit Excel" />
        <MenuItem onAction="#manageArchive" mnemonicParsing="false"
          text="Archief opschonen" />
        <MenuItem onAction="#settings" mnemonicParsing="false" text="Instellingen" />
      </items>
    </Menu>
  </menus>
</MenuBar>
